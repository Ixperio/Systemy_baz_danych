@model IEnumerable<WebApplication4.Models.Funkcjonariusz>

@{
    ViewBag.Title = "Spis funkcjonariuszy";
}

<h2>Funkcjonariusze</h2>

<p>
    @Html.ActionLink("Dodaj funkcjonariusza", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Stopien.Nazwa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Obywatel.Pesel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.adres_Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Wyksztalcenie)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Stopien.Nazwa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telefon)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Obywatel.Pesel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adres_Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wyksztalcenie)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.id_Funkcjonariusza }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.id_Funkcjonariusza }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.id_Funkcjonariusza })
        </td>
    </tr>
}

</table>
