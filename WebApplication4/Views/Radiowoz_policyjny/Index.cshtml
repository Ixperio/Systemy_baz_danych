@model IEnumerable<WebApplication4.Models.Radiowoz_policyjny>

@{
    ViewBag.Title = "Radiowozy Policyjne";
}

<h2>Radiowozy Policyjne</h2>

<p>
    @Html.ActionLink("Dodaj nowy", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Samochod.Marka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nr_Rejestracyjny)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rocznik)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ubezpieczenie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Przebieg)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Przeglad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Samochod.Marka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nr_Rejestracyjny)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rocznik)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ubezpieczenie)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Przebieg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Przeglad)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.Id }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
